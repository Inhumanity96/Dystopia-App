script/source = "extends \"res://singletons/Debug.gd\"

\"\"\"
it Inherits from the Debug singleton. COnnect it to a signal from the Debug singleton
\"\"\"
#lists of labels being used
onready var music_label 
onready var player_label 
onready var ram_label
onready var fps_label
onready var enemy_label
onready var network_label
onready var comics_label



# Called when the node enters the scene tree for the first time.
func _ready():
	#call the label node and save it as a variable 
	music_label =$CanvasLayer/Control/MarginContainer/VBoxContainer/Label
	player_label =$CanvasLayer/Control/MarginContainer/VBoxContainer/Label2
	ram_label =$CanvasLayer/Control/MarginContainer/VBoxContainer/Label3
	fps_label =$CanvasLayer/Control/MarginContainer/VBoxContainer/Label4
	enemy_label =$CanvasLayer/Control/MarginContainer/VBoxContainer/Label5
	network_label =$CanvasLayer/Control/MarginContainer/VBoxContainer/Label6
	comics_label =$CanvasLayer/Control/MarginContainer/VBoxContainer/Label7

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(_delta):
	if debug_panel != null:
		music_label.text =Debug.Music_debug
		player_label.text =Debug.Player_debug
		ram_label.text =(Debug.Ram_debug) 
		fps_label.text =Debug.FPS_debug
		enemy_label.text =Debug.Enemy_debug
		network_label.text = 'Network Debug : Null'
		comics_label.text = Debug.Comics_debug
	elif debug_panel == null:
		push_error(\"Error getting Debug singleton\")
