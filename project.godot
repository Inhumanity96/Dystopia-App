"class": "DialogBox",
"language": "GDScript",
"path": "res://scenes/UI & misc/Dialog_box.gd"
}, {
"base": "KinematicBody2D",
"class": "Enemy",
"language": "GDScript",
"path": "res://scenes/characters/Enemy.gd"
}, {
"base": "Area2D",
"class": "Exit",
"language": "GDScript",
"path": "res://scenes/UI & misc/Exit.gd"
}, {
"base": "Area2D",
"class": "Idol",
"language": "GDScript",
"path": "res://scenes/props/idol.gd"
}, {
